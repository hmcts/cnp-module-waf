{
  "$schema":
    "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "state": {
      "type": "array",
      "defaultValue": []
    },
    "source": {
      "type": "object"
    },
    "applicationGatewayID": {
      "type": "string"
    }
  },
  "variables": {
    "instance": [
      {
        "matchVariable": "[parameters('source').matchVariable]",
        "selectorMatchOperator": "[parameters('source').selectorMatchOperator]",
        "selector": "[parameters('source').selector]"
      }
    ]
  },
  "resources": [],
  "outputs": {
    "collection": {
      "type": "array",
      "value": "[concat(parameters('state'), variables('instance'))]"
    }
  }
}
